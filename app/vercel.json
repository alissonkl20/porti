{
  "headers": [
    {
      "source": "/(.*)\\.(js|css|svg|png|jpg|jpeg|webp|woff|woff2|ttf|eot|ico|ttf|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(html|json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}